year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)
bibentry(bibtype = "Manual", title = "{FPTP2AV}: Simulate Election Outcomes for AV Given FPTP Votes", author = person("Daniel", "Marcelino"), year = year, note = note, url = "http://github.com/danielmarcelino/FPTP2AV")
